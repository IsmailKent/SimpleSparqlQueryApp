<h1 style=" text-align: center">database gui</h1>
<div style=" width:100%; display:flex; flex-wrap:wrap">
    <div style="width: 50%">
        <h2 style=" text-align: center">Available Classes of type owl:Class <br><br><br> </h2>
        <div style="max-height:500px; overflow:scroll;">
            {{#each results}}
            <form action="" method="post">
                <button style="width:90%; margin:5px" class="btn btn-secondary" type="value" name="name"
                    value="{{this.result.subject.value}}">
                   <!-- <em><strong>TYPE: </strong></em>{{this.subject.type}} <br> -->
                    <em><strong>VALUE:</strong></em> {{this.cleanResult}}

                </button>
            </form>
            {{/each}}
        </div>
    </div>
    <div style="width: 50%">
        <h2 style=" text-align: center">Predicates and Objects of Selected item <br> {{this.subject}} </h2>
        <ul class="list-group">
            {{#each preAndObj}}
            <li class="list-group-item"><strong>Predicate:</strong>{{this.predicate}} <br><strong>Object:</strong>
                {{this.object}} </li>
            {{/each}}

        </ul>
    </div>
</div>